{
  "name": "youkeepit-website",
  "version": "0.1.0",
  "private": true,
  "description": "Secure YouKeepIt website with Next.js 14+ and Supabase authentication",
  "author": "Your Name <your.email@company.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "security-check": "node scripts/security-check.js",
    "pre-deploy": "npm run security-check && npm run type-check && npm run lint && npm run build",
    "deploy": "vercel --prod",
    "deploy-safe": "npm run pre-deploy && npm run deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "postinstall": "npm audit --audit-level moderate",
    "clean": "rm -rf .next out node_modules/.cache",
    "db:migrate": "echo 'Run migrations in Supabase SQL Editor'",
    "db:seed": "echo 'Run seed data in Supabase SQL Editor'",
    "env:pull": "vercel env pull .env.local",
    "env:check": "node scripts/env-check.js",
    "logs": "vercel logs",
    "logs:follow": "vercel logs --follow"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@supabase/supabase-js": "^2.50.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.17.0",
    "jose": "^5.9.6",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "next": "^14.0.0",
    "next-auth": "^4.24.10",
    "next-themes": "^0.4.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-intersection-observer": "^9.16.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "autoprefixer": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.0.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "nextjs",
    "react",
    "supabase",
    "authentication",
    "secure",
    "admin-panel",
    "typescript",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youkeepit-website.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/youkeepit-website/issues"
  },
  "homepage": "https://youkeepit.com",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run type-check",
      "pre-push": "npm run security-check"
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "overrides": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}